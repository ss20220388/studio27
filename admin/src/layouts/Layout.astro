---
const { title } = Astro.props;
import "../styles/global.css";
import Sidebar from "../components/Sidebar.astro";
---
<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>{title}</title>
    </head>
    <body>
        <div class="md:flex ">
            <Sidebar />
            <slot />
        </div>
        
    </body>
</html>